<?xml version="1.0" encoding="UTF-8"?>

<excelConfig>
	<parameters>
		<!--保存时使用的模板,根据配置文件生成的excel,需要将数据保存为excel到本地,然后有save方法,再生成sql,保存到本地-->
		<parameter name="outputFilePath" value=""/>
		<parameter name="template_FilePath" value="C:\00_Project\Jeesite070310\src\main\java\com\thinkgem\jeesite\common\excelConfig\xlsx\PhoneList.xlsx"/>
	</parameters>
	<models cls="">
		<!--
			单表查询显示
		-->
		<model  id="phonelist">
			<parameters>
				<!--<parameter name="Sno" class="java.lang.String" desc="sno" value="1"/>-->
			</parameters>
			<SQLStrs>
				<sqlStr>
					<![CDATA[
SELECT
row_number()over(order by c.name,a.EName ) innum,'' 'test1',
c.name 'Department', a.EName 'user',a.ENameEN 'userEn',a.CompanyPhone,a.mail,a.Phone
 FROM IT_User a
JOIN sys_user b ON a.EmployeeID = b.no AND a.EmployeeID is NOT NULL AND b.del_flag='0' AND b.login_flag='1'
JOIN sys_office c ON b.office_id = c.id
order by c.name,a.EName
					]]>
				</sqlStr>
			</SQLStrs>
		</model>

	</models>
	<views   action="query">
		<view modelid="phonelist" sheetName="sheet1"   top="1" left="1">
			<propertys>
				<property 	cellIndex="0,1" feild="user" 	type="java.lang.String"  />
				<property 	cellIndex="0,1" feild="userEn"   type="java.lang.String" />
				<property 	cellIndex="0,1"	feild="Department" 	type="java.lang.String" />
				<property 	cellIndex="0,1"	feild="CompanyPhone" 	type="java.lang.String" />
				<property 	cellIndex="0,1"	feild="mail" 	type="java.lang.String" />
				<property 	cellIndex="0,1"	feild="Phone" 	type="java.lang.String" />
				<property 	cellIndex="0,1"	feild="test1" 	type="java.lang.String" />
				<property 	cellIndex="0,1"	feild="innum" 	type="java.lang.String" />
			</propertys>
		</view>
	</views>
</excelConfig>