<?xml version="1.0" encoding="UTF-8"?>

<excelConfig>
	<parameters>
		<!--保存时使用的模板,根据配置文件生成的excel,需要将数据保存为excel到本地,然后有save方法,再生成sql,保存到本地-->
		<parameter name="outputFilePath" value=""/>
		<parameter name="template_FilePath" value="report2.xlsx"/>
		<parameter name="temlateSyntax" value="false"/>
	</parameters>
	<models cls="">
		<!--
         **********************
		 此报告为员工统计报告 入职年限时间等分析性别比例分析
		*********************
		-->
		<model  id="statistic">
			<parameters>
				<parameter name="Sno" class="java.lang.String" value="1"/>
			</parameters>
			<SQLStrs>
				<sqlStr>
					<![CDATA[
					SELECT
					F_PAR4  '姓名',
					F_PAR5  '性别',
					F_PAR26 '年龄',
					F_PAR34 '入职日期',
					datediff(yy,CONVERT(DATETIME,F_PAR34,121),getdate())  YEAR
					FROM dbo.T_OBJ_TYPE_REL
					WHERE F_TYPEID = 'A0'
					]]>
				</sqlStr>
			</SQLStrs>
		</model>
	</models>
	<views   action="query">
		<view modelid="statistic" sheetName="sheet2"   top="0" left="0">
			<propertys>
				<property 	cellIndex="0,0" feild="姓名" 	 type="java.lang.String"  />
				<property 	cellIndex="0,1"	feild="年龄" 	 type="java.lang.Integer" />
				<property 	cellIndex="0,1"	feild="YEAR" 	 type="java.lang.Integer" />
				<property 	cellIndex="0,1" feild="性别"      type="java.lang.String" />
				<property 	cellIndex="0,1"	feild="入职日期" 	 type="java.lang.String" />
			</propertys>
		</view>
	</views>

</excelConfig>